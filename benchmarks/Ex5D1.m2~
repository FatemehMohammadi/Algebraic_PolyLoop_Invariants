restart
t1 = cpuTime()
K = QQ
R= K[ x_1 .. x_5]
f1= x_2
f2 = x_1+x_2
f7 = x_3+x_4*x_1+x_5*x_2
a_1 = 0
a_2 =1
g = sub(f7, {x_1=>a_1, x_2=>a_2})
T = ideal(g)
i=0;
while i< 3 do (
    print i;
    y_1 = sub(f1,{x_1=>a_1, x_2=>a_2});
    y_2 = sub(f2,{x_1=>a_1, x_2=>a_2});
    a_1 = y_1;
    a_2 = y_2;
    a_3 = y_3;
    g = sub(f7, {x_1=>a_1, x_2=>a_2});
    T = T +ideal(g);
    i=i+1;
    )
T = trim T
t2= cpuTime()
S={}
n = numgens T
m = 3
i =0
while i< n do(
    U_i =S;
    j =1;
    k = 2;
    a_1 =1;
    while k<m+2 do (
	a_k =0;
	k=k+1;
	);
    while j <  m+1 do(
	b=sub(T_i,{x_3=>a_1, x_4=>a_2, x_5=>a_3});       
	U_i = join(U_i, {b});   
	a_j = a_(j+1);
    	a_(j+1)= a_(j+1)+1;
	j = j+1;
	);
    i = i+1;
    )  
M = matrix{U_0, U_1}
i=0;
while i< n-2 do(
    M = M||matrix{ U_(i+2)};
    i = i+1;
    )

A=ker M
A = generators A
a = matrix{{1},{x_1},{x_2}}
P = transpose(a)*A
n = numgens source P
j =0;
while j< n do(
    X_0 = ideal(P_(0,j));
    j=j+1;
    X_1=sub(X_0, {x_1 =>f1,x_2 =>f2})+X_0;
    X_1= radical X_1;
    i = 0;
    while  X_i != X_(i+1)  do (
    	i = i+1;
    	print i;
    	X_(i+1) = sub(X_i, {x_1 =>f1,x_2 =>f2}) + X_0 ;
    	print 100;
	);
    S=  sub(X_i,{x_1=>0, x_2=>1});
    S= trim S;
    print S;
)
t3= cpuTime()
