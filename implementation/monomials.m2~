monomial =(d,n)-> (
    K =QQ;
    R = K[x_1..x_n];
    S = {1};
    i =1 ;
    while i< n+1 do (
    	S = join(S,x_i);
    	i = i+1;
    	);
    C = S; 
    i =1;
    while i< d do(
    	l = length C;
    	print l;
    	B = {};
    	j = 0;
    	while j< l do(
	    t =0;
	    while t<n+1 do(
	    	b = C_j*S_t;
	    	t=t+1;
	    	B =join(B, {b});
	    	);
	    j= j+1;
	    );
    	C = B;
    	i = i+1;
    ); 
    C = set C;
    C = toList C       
    )

end --

Generating general polynomials of degree d with n variables


f = d ->(
    K =QQ;
    R = K[x_1];
    x_1*d     
    
    )


